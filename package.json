{
  "name": "nuxt-add",
  "version": "0.0.3",
  "description": "Install nuxt modules with ease",
  "homepage": "https://github.com/xanderbarkhatov/nuxt-vercel-analytics#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xanderbarkhatov/nuxt-vercel-analytics.git"
  },
  "author": {
    "name": "Alexander B.",
    "email": "mail@xanderbarkhatov.com",
    "url": "https://xanderbarkhatov.com"
  },
  "keywords": [
    "nuxt",
    "modules",
    "nuxt-modules",
    "cli"
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts-resolve",
    "release": "pnpm build && changelogen --release && npm publish && git push --follow-tags"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.4",
    "changelogen": "^0.5.3",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "consola": "^3.1.0",
    "execa": "^7.1.1",
    "magicast": "^0.2.9",
    "ofetch": "^1.0.1",
    "ora": "^6.3.1",
    "prompts": "^2.4.2"
  }
}
